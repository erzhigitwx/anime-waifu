{
  "name": "anime-gemini-bot",
  "version": "1.0.0",
  "description": "Anime character chatbot powered by Google Gemini",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest",
    "postinstall": "npm run build"
  },
  "keywords": [
    "telegram",
    "bot",
    "anime",
    "gemini",
    "ai"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.7.1",
    "axios": "^1.12.2",
    "dotenv": "^16.6.1",
    "mongoose": "^8.18.2",
    "telegraf": "^4.16.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.127",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
